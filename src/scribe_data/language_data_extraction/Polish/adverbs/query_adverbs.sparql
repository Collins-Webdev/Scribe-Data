# tool: scribe-data
# All Polish (Q809) adverbs and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  ?lexeme  # unique ID for the data entry
  ?adverb  # lemma or label of the ID

WHERE {
  ?lexeme dct:language wd:Q809 ;  # Polish language
    wikibase:lexicalCategory wd:Q380057 ;  # Adverbs
    wikibase:lemma ?adverb .
}

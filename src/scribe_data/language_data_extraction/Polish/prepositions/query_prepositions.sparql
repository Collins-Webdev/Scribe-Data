# tool: scribe-data
# All Polish (Q809) prepositions and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  ?lexeme  # unique ID for the data entry
  ?preposition  # lemma or label of the ID

WHERE {
  ?lexeme dct:language wd:Q809 ;  # Polish language
    wikibase:lexicalCategory wd:Q4833830 ;  # Propositions
    wikibase:lemma ?preposition .
}
